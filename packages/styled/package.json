{
  "name": "stylee",
  "version": "0.0.1",
  "description": "Stylee is an easy and simple CSS framework.",
  "scripts": {
    "git:push": "git add . && git commit -m 'Update codes' && git push"
  },
  "author": "Tetritz <tetrtiz.com@gmail.com>",
  "bugs": {
    "url": "https://github.com/tetritz/stylee/issues"
  },
  "keywords": [
    "css",
    "framework",
    "react",
    "style",
    "styled",
    "stylee",
    "tetritz"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tetritz/stylee.git",
    "directory": "packages/styled"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@devtree/react-pack": "^0.0.7",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "babel-plugin-styled-components": "^1.12.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "styled-components": "^5.2.1"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@devtree/react-pack/eslint/base.js"
    ]
  },
  "prettier": "@devtree/react-pack/prettier"
}
